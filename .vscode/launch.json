    "version": "0.2.0",
    "configurations": [
        {
            "name": "Iris",
            "type": "gdb",
            "request": "launch",
            "target": "./pivotter",
            "arguments": "-x0 -y2 -u4 -s data/iris.csv",
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText"
        },
        {
            "name": "Iris (output.csv)",
            "type": "gdb",
            "request": "launch",
            "target": "./pivotter",
            "arguments": "-x0 -y2 -u4 -s data/output.csv",
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText"
        },
        {
            "name": "it.cel.sets.p2_subset",
            "type": "gdb",
            "request": "launch",
            "target": "./pivotter",
            "arguments": "-x2 -y3 -u1 data/it.cel.sets.p2_subset.csv",
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText"
        },
        {
            "name": "it.cel.sets.p2_subset (output.csv)",
            "type": "gdb",
            "request": "launch",
            "target": "./pivotter",
            "arguments": "-x2 -y3 -u1 data/output.csv",
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText"
        },
    ]
}